{
  "name": "eslint-prefs",
  "type": "module",
  "version": "2.3.2",
  "packageManager": "pnpm@8.10.2",
  "description": "NBE's ESLint prefs",
  "author": "Naser Aleisa <aleisa.naser@gmail.com> (https://github.com/thenbe/)",
  "license": "MIT",
  "homepage": "https://github.com/thenbe/eslint-prefs",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "pnpm run stub && eslint .",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "eslint-plugin-tailwindcss": "3.13.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@antfu/ni": "^0.21.12",
    "@types/eslint": "^8.56.3",
    "@types/fs-extra": "^11.0.4",
    "bumpp": "^9.3.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
